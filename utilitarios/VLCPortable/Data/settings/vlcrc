[visual] # Visualizer filter

# Effects list (string)
#effect-list=spectrum

# Video width (inteiro)
#effect-width=800

# Video height (inteiro)
#effect-height=500

# FFT window (string)
#effect-fft-window=flat

# Kaiser window parameter (float)
#effect-kaiser-param=3.000000

# Show 80 bands instead of 20 (booleano)
#visual-80-bands=1

# Draw peaks in the analyzer (booleano)
#visual-peaks=1

# Enable original graphic spectrum (booleano)
#spect-show-original=0

# Draw the base of the bands (booleano)
#spect-show-base=1

# Base pixel radius (inteiro)
#spect-radius=42

# Spectral sections (inteiro)
#spect-sections=3

# V-plane color (inteiro)
#spect-color=80

# Draw bands in the spectrometer (booleano)
#spect-show-bands=1

# Show 80 bands instead of 20 (booleano)
#spect-80-bands=1

# Number of blank pixels between bands. (inteiro)
#spect-separ=1

# Amplification (inteiro)
#spect-amp=8

# Draw peaks in the analyzer (booleano)
#spect-show-peaks=1

# Peak extra width (inteiro)
#spect-peak-width=61

# Peak height (inteiro)
#spect-peak-height=1

